package com.first;
import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String[] args) throws Exception {
        BufferedReader rew = new BufferedReader(new InputStreamReader(System.in));
        int a = Integer.parseInt(rew.readLine());
        int b = Integer.parseInt(rew.readLine());
        int c = Integer.parseInt(rew.readLine());
        int d = Integer.parseInt(rew.readLine());
        if (a==b && b==c &&c==d){
            System.out.println(b);
        }
        if(a<b)
            System.out.println(b);
        else if (b<c)
            System.out.println(c);
        else if(c<d)
            System.out.println(d);
        else if(a>d)
            System.out.println(a);

    }
}
